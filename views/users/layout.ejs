				<!-- Main navigation -->
				<ul class="navigation">
					<li class="<%= (active_page == 'dashboard' ? 'active' : '')%>"><a href="/dashboard"><span>Dashboard</span><i class="icon-screen2"></i></a></li>
          <li class="<%= (active_page == 'orgs' ? 'active' : '')%>">
						<a class="expand <%=(us.startsWith(active_page,'org_')) ? 'level-open' : 'level-closed' %>" id="<%=(us.startsWith(active_page,'org_')) ? 'second-level' : '' %>" href="#"><span>Organizations</span> <i class="icon-people"></i></a>
						<ul>
   							<% user.github.orgs.forEach(function(org){ %>
  								<li class="<%= (active_page == 'org_' + org.login ? 'active' : '')%>"><a href="/org/<%=org.login%>"><%=org.login%></a></li>
  							<% }) %>

						</ul>
					</li>
          <li class="<%= (active_page == 'repos' ? 'active' : '')%>">
						<a class="expand <%=(us.startsWith(active_page,'repo_')) ? 'level-open' : 'level-closed' %>" id="<%=(us.startsWith(active_page,'repo_')) ? 'second-level' : '' %>" href="#"><span>Repositories</span> <i class="icon-people"></i></a>
						<ul>
   							<% user.github.repos.forEach(function(repo){ %>
  								<li class="<%= (active_page == 'repo_' + repo.owner.login + '_ ' + repo.name ? 'active' : '')%>"><a href="/repo/<%=repo.owner.login%>/<%=repo.name%>"><%=repo.name%></a></li>
  							<% }) %>

						</ul>
					</li>
