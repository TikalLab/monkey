<table>
  <tr>
    <th>repo</th>
    <th>branch</th>
    <th>file</th>
    <th>keys found</th>
  </tr>
<% local_scan.matches.forEach(function(match){ %>
  <tr>
    <td><%=match.repo%></td>
    <td><%=match.branch%></td>
    <td><%=match.file%></td>
    <td><%=match.matches[0]%></td>
  </tr>
<% }) %>
</table>
